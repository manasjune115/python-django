<script>
    function showhide(id){
        if (document.getElementById){
            obj = document.getElementById(id);
            if (obj.style.display == "none"){
                obj.style.display = "table";
            } else {
                obj.style.display = "none";
	            }
        }
    }
function openleture(no){

	emailwindow= dhtmlmodal.open('EmailBox', 'iframe', 'emp/'+no  , 'Employee history', 'width=550px,height=450px,center=1,resize=0,scrolling=1')

} 

function openemail(no){
	emailwindow= dhtmlmodal.open('EmailBox', 'iframe', 'phonehis/'+no  , 'phone history', 'width=550px,height=450px,center=1,resize=0,scrolling=1')

} 

function opendesign(str1){
	var str=str1;
	emailwindow= dhtmlmodal.open('EmailBox', 'iframe', 'designationhis/'+str  , 'designation history', 'width=550px,height=450px,center=1,resize=0,scrolling=1')

} 

function opendept(no){
	emailwindow= dhtmlmodal.open('EmailBox', 'iframe', 'deptinfo/'+no  , 'phone history', 'width=550px,height=450px,center=1,resize=0,scrolling=1')

} 

function open_order(str1){
	var str=str1;
	emailwindow= dhtmlmodal.open('EmailBox', 'iframe', 'fileupload/'  , 'Order', 'width=550px,height=450px,center=1,resize=0,scrolling=1')

} 
	
</script>


